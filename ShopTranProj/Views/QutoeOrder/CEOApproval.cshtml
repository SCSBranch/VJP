        //var data = [{  part_name: "", pilot_qty: "", cast_draw_no: "", mach_draw_no: "", status_code: "2",  mode_flag: "S" },
        //{part_name: "", pilot_qty: "", cast_draw_no: "", mach_draw_no: "", status_code: "2",  mode_flag: "S" },
        //{  part_name: "", pilot_qty: "", cast_draw_no: "", mach_draw_no: "", status_code: "2", mode_flag: "S" }];

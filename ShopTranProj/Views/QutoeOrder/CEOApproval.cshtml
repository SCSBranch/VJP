                            combo_editor_man(container, "cmb_" + options.field, eval(options.field + "_list"), options.field, "status_code", "grid_ceo_approval")
                        },
                        width: 100,
                    },
                    {
                        command: [
                            {
                                name: "Save",
                                text: "",
                                id: "Save",

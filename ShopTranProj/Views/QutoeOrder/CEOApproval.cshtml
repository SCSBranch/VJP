<script>
    function Customer_onChange(arg) {
        $('#Customer_popup').click();
        Load_Customer_popup(customer_rowid);
    }
    function RFQ_onChange() {
        $('#rfq_entry_popup').click();
        rfq_number = $("#txtRFQNO").val();
        var grid = $("#grid_ceo_approval").data("kendoGrid");
        var selectedItem = grid.dataItem(grid.select());
        cust_name = selectedItem.cust_name;
        Load_RFQ_popup(rfq_number);
    }

    var customer_name_list = '';
    var poc_list = '';
    var status_list = '';
    var action_taken_list = '';
    function Approval_grid(data) {
        try {
            gridRowID = -1;
            data = addRandomNum(data);
            $("#grid_ceo_approval").kendoGrid({
                dataSource: {
                    data: data,
                    pageSize: 20,
                    schema: {
                        model: {
                            fields: {
                                rfq_no: {
                                    type: "string",
                                    editable: false,
                                },
                                action_taken_prod: {
                                    type: "string",
                                    editable: false,
                                },
                                action_taken_fai: {
                                    type: "string",
                                    editable: false,
                                },

                                part_name: {
                                    type: "string",
                                    editable: false,
                                },
                                cust_name: {
                                    type: "string",
                                    editable: false,
                                },

                                ceo_remarks_prod: {
                                    type: "string",
                                    editable: false,
                                },
                                ceo_remarks_fai: {
                                    type: "string",
                                    editable: false,
                                },

                                status_code: {
                                    type: "string",
                                    editable: false,
                                },
                                status: {
                                    type: "string",
                                    editable: false,
                                },
                            }
                        }
                    },
                },
                dataBound: function (o) {
                    reset_Selected_GridRows("grid_ceo_approval", o);
                },
                //height: 250,
                filterable: true,
                scrollable: true,
                sortable: {
                    mode: "multiple",
                    dir: "asc"
                },
                pageable: false,
                selectable: true,
                //change: selectRow,
                columns: [
                    {
                        field: "cust_rowid",
                        title: "Cust Rowid",
                        width: "150px",
                        hidden: true
                    },
                    {
                        field: "rfq_no",
                        title: "RFQ No/ Enquiry No",
                        width: "150px",
                    },
                    {
                        field: "cust_name",
                        title: "Customer Name",
                        width: "125px",
                    },

                    {
                        field: "action_taken_prod",
                        title: "Action taken (Production)",                        
                        width: "170px",
                    },
                    {
                        field: "ceo_remarks_prod",
                        title: "CEO Remarks (Production)",
                        width: "180px",
                    },
                    {
                        field: "action_taken_fai",
                        title: "Action taken FAI)",                       
                        width: "150px",
                    },
                    
                    {
                        field: "ceo_remarks_fai",
                        title: "CEO Remarks (FAI)",
                        width: "150px",
                    },
                    {
                        field: "status_code",
                        title: "Status",
                        hidden: true,
                        width: "150px",
                    },
                    {
                        field: "status",

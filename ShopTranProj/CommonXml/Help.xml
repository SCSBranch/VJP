<?xml version="1.0" encoding="utf-8" ?>
<root>

  
 

  <search id="P01" description="Dependent Code Search">
    <inputs table="dependent_code_vw">
      <input    seqno="1" column="description" 	   coldesc="Dependent Code" 	     datatype="A" entflg="" defval="" defcondt="like"/>
    </inputs>
    <outputs orderby="1,2">
      <output  	seqno="1" column="code"	  coldesc="Code"  	   textalign="left"/>
      <output  	seqno="2" column="description"	  coldesc="Description"  	hidden="true"   textalign="left"/>
    </outputs>
  </search>

  <search id="P02" description="Customer ID search">
    <inputs table="CustomerId_vw">
      <input    seqno="1" column="user_id"  coldesc="User ID" datatype="A" entflg="Y" defval="" defcondt="like"/>
    </inputs>
    <outputs orderby="1,2">
      <output  	seqno="1" column="user_id"	 	      coldesc="User ID"  	 textalign="left"/>
      <output  	seqno="2" column="user_name" 	coldesc="User Name"	 textalign="left"/>
    </outputs>
  </search>

  

  <condition>
    <condt>equal to</condt>
    <condt>like</condt>
    <condt>greater than</condt>
    <condt>not equal to</condt>
    <condt>not like</condt>
  </condition>

</root>
